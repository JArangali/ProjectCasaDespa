@{
    ViewData["Title"] = "Reset Password";
}

<!-- Reset Password Section -->
<section id="rp">
    <div id="backbuttondiv">
        <button id="fpbackbutton"> <a id="fpabackbutton" asp-area="" asp-controller="Account" asp-action="ForgotPassword" class="getstarted"> <i class="material-icons">chevron_left</i> </a> </button>
    </div>
    <form>
        <div id="rpform">
            <div id="rptitle">
                <div id="rptitle1">
                    <img id="sqlogoimg" src="~/img/sqlogo.png" alt="sqlogo"><h2 id="rpheading"> ADMIN CODE </h2>
                </div>
            </div>
            <div id="rpformcontent">
                <div id="rpinputfields">
                    <h4 id="rptext">Admin Code</h4>
                    <input type="text" id="rpanswerinput" placeholder="Please Provide Admin Code">
                </div>
            </div>
            <div id="rpbuttonscontainer">
                <button id="rpnextbutton"><a id="rpnext" asp-area="" asp-controller="Account" asp-action=""> NEXT </a> </button>
            </div>
        </div>
    </form>
</section>
